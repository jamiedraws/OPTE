    </httpHandlers>
  </system.web>
  <system.web.extensions/>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Strict-Transport-Security" value="max-age=8995000"/>
        <add name="X-Content-Type-Options" value="nosniff"/>
        <add name="X-XSS-Protection" value="1"/>
        <add name="X-Frame-Options" value="SAMEORIGIN"/>
